// run-pass

// Testing operator precedence.


fn main() {
    assert_eq_int(2 + 3**2*3 + 4, 33);
    assert_eq_int(2 + 3**(2*3) + 4, 735);
    assert_eq_int((1 + (2*(3**4))), 163);
    assert_eq_int(3**5 + 3*4 + 9/3**2, 256);
    assert_eq_int(3**5 + 3*4 + (9/3)**2, 264);
    assert_eq_int(3**5 + 3*4 - (9/3)**2, 246);
    assert_eq_bool((5 + 3**2 - 3) >  10, true);
    assert_eq_bool((5 + 3**2 - 3) >  11, false);
    assert_eq_bool((5 + 3**2 - 3) >= 11, true);
    assert_eq_bool((5 + 3**2 - 3) >= 12, false);
    assert_eq_bool((5 + 3**2 - 3) <  11, false);
    assert_eq_bool((5 + 3**2 - 3) <  11, false);
    assert_eq_bool((5 + 3**2 - 3) <  12, true);
    assert_eq_bool((5 + 3**2 - 3) <= 11, true);
    assert_eq_bool((5 + 3**2 - 3) <= 10, false);
    assert_eq_bool((5 + 3**2 - 3) <= 12, true);
}

